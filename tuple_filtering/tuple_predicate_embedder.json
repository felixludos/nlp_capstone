{
  "dataset_reader": {
    "type": "tuple_reader",
    "lazy": false,
    "limit": 100,
    "token_indexers": {
      "tokens": {
        "type": "single_id",
        "lowercase_tokens": true
      }
    }
  },
  "train_data_path": "data/aristo.txt",
  "model": {
    "type": "tuple_predicate_embedder",
    "entity_embedder": {
      "token_embedders": {
        "tokens": {
          "type": "embedding",
          "embedding_dim": 300,
          "trainable": false
        }
      }
    },
    "entity_seq2vec": {
      "type": "lstm",
      "input_size": 300,
      "hidden_size": 300,
      "num_layers": 1,
      "bidirectional": true
    },
    "predicate_embedder": {
      "token_embedders": {
        "tokens": {
          "type": "embedding",
          "embedding_dim": 300,
          "trainable": false
        }
      }
    },
    "predicate_seq2vec": {
      "type": "lstm",
      "input_size": 300,
      "hidden_size": 300,
      "num_layers": 1,
      "bidirectional": true
    },
    "entity_output_layer": {
      "input_dim": 1200,
      "num_layers": 2,
      "hidden_dims": [
        600,
        300
      ],
      "dropout": [
        0.2,
        0.0
      ],
      "activations": [
        "relu",
        "linear"
      ]
    },
    "predicate_output_layer": {
      "input_dim": 600,
      "num_layers": 2,
      "hidden_dims": [
        600,
        300
      ],
      "dropout": [
        0.2,
        0.0
      ],
      "activations": [
        "relu",
        "linear"
      ]
    }
  },
  "iterator": {
    "type": "basic",
    "batch_size": 100
  },
  "trainer": {
    "num_epochs": 40,
    "patience": 5,
    "grad_norm": 5.0,
    "cuda_device": 0,
    "optimizer": {
      "type": "adam",
      "lr": 0.001
    }
  }
}